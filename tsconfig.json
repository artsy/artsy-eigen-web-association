{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "types": ["@cloudflare/workers-types"]
  },
  "include": [
    "worker.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
